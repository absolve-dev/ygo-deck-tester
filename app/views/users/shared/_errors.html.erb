<% if devise_error_messages! && devise_error_messages! != "" %>
  <% saved_key = "" %>
  <% devise_error_messages!.each do |key, value| %>
    <% if key != saved_key %>
        <div class="alert alert-warning"><%= key %> <%= value %><button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">&times;</span></button></div>
    <% end %>
    <% saved_key = key %>
  <% end %>
<% end %>